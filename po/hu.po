#: ../panel-plugin/battery.c:494
msgid "(AC on-line)"
msgstr "(Energiaellátás elérhető)"

#: ../panel-plugin/battery.c:503
#, c-format
msgid "%d%% (%02d:%02d) remaining"
msgstr "%d%% (%02d.%02d) van hátra"

#: ../panel-plugin/battery.c:505
