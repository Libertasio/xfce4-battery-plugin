msgid "(No battery, AC on-line)"
msgstr "(バッテリなし、AC 接続)"

#: ../panel-plugin/battery.c:502
msgid "(Charging from AC)"
msgstr "(AC から充電中)"

#: ../panel-plugin/battery.c:502
msgid "(AC on-line)"
msgstr "(AC 接続)"

#: ../panel-plugin/battery.c:512
#, c-format
