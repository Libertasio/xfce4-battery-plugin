#, c-format
msgid "%02d:%02d remaining"
msgstr "剩余 %02d:%02d"

#: ../panel-plugin/battery.c:515
#, c-format
msgid "%d%% remaining"
msgstr "剩余 %d%%"

#: ../panel-plugin/battery.c:517
#, c-format
