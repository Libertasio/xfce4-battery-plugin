#: ../panel-plugin/battery.c:499
msgid "(No battery, AC on-line)"
msgstr "（没有电池，已连接至交流电源）"

#: ../panel-plugin/battery.c:501
msgid "(Charging from AC)"
msgstr "（从交流电源充电）"

#: ../panel-plugin/battery.c:501
msgid "(AC on-line)"
msgstr "（已连接至交流电源）"

#: ../panel-plugin/battery.c:511
#, c-format
